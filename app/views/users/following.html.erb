<div class = "container">
	<h1>フォロー一覧</h1>
	<% @follows.each do |following| %>
		<%= attachment_image_tag following, :profile_image, :fill, 60, 60, fallback: "no_image.jpg",size: '60x60' %>
		<strong><%= link_to following.name, user_path(following.id) %></strong>
		<%= following.introduction %>
	<% end %>
</div>