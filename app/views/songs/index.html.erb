<div class = "container">
	<h1>曲一覧</h1>
	<% @songs.each do |song| %>
    	<%= link_to song_path(song.id) do %>
    		<div class="row">
       			<div class = "song-index">
		 			<%= song.name %>
		 			<%= song.artist %></br>
		 			<%= song.description %>
				</div>
			</div>
		<% end %>
	<% end %>
</div>
